<%= form_for [@portfolio, @coin_portfolio] do |f| %>
  <%= f.fields_for :coin do |coin_fields| %>
    <%= coin_fields.label :name %>:
    <%= coin_fields.collection_select :id, Coin.all, :id, :name %>
  <% end %>
  <%= f.label :quantity %>:
  <%= f.number_field :quantity %>
  <%= f.submit 'Continue' %>
<% end %>